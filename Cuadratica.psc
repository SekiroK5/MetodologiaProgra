Proceso Cuadratica
	Definir a, b, c Como Real;
	Definir x1, x2 Como Real;
	Escribir "Ingresa tres valores distintos";
	leer a,b,c;
	x1<-(-b+raiz(b^2-4*a*c))/(2*a);
	x2<-(-b-raiz(b^2-4*a*c))/(2*a);
	Escribir "Los resultados de la ecuación cuadratica son:";
	Escribir "X1= ", x1;
	Escribir "X2= ", x2;
FinProceso